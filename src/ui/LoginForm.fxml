<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="251.0" prefWidth="307.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <AnchorPane prefHeight="251.0" prefWidth="336.0">
         <children>
            <TextField fx:id="tfUserName" layoutX="22.0" layoutY="36.0" prefHeight="35.0" prefWidth="249.0" promptText="%username">
               <font>
                  <Font size="16.0" />
               </font></TextField>
            <TextField fx:id="tfPassword" layoutX="22.0" layoutY="127.0" prefHeight="35.0" prefWidth="249.0" promptText="%password">
               <font>
                  <Font size="16.0" />
               </font></TextField>
            <Label fx:id="lblUsernameError" layoutX="22.0" layoutY="71.0" prefHeight="35.0" prefWidth="249.0" textFill="RED" />
            <Label fx:id="lblPasswordError" layoutX="23.0" layoutY="162.0" prefHeight="35.0" prefWidth="249.0" textFill="RED" />
            <Label fx:id="lblLocation" layoutX="12.0" layoutY="209.0" prefHeight="35.0" prefWidth="144.0">
               <font>
                  <Font size="14.0" />
               </font></Label>
            <Label layoutX="23.0" layoutY="19.0" text="%username" />
            <Label layoutX="23.0" layoutY="112.0" text="%password" />
            <Button fx:id="btnOk" layoutX="160.0" layoutY="209.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="59.0" text="%ok">
               <font>
                  <Font size="15.0" />
               </font></Button>
            <Button fx:id="btnCancel" layoutX="224.0" layoutY="209.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="75.0" text="%cancel">
               <font>
                  <Font size="14.0" />
               </font></Button>
         </children>
      </AnchorPane>
   </children>
</VBox>
